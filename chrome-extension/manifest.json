{
  "manifest_version": 3,
  "name": "browserman",
  "description": "all your cookies are belong to us",
  "version": "1.0",
  "action": {
    "default_title": "browserman",
    "default_popup": "index.html"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "cookies",
    "activeTab",
    "tabs"
   ]
}
